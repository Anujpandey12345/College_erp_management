{
    "version": 2,
    "builds": [{
            "src": "CodingIndiaErp/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.11",
                "includeFiles": "staticfiles_build/static/**",
                "installCommand": "pip install -r requirements.txt --no-deps"
            }
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "staticfiles_build/static"
            }
        }
    ],
    "routes": [{
            "src": "/static/(.*)",
            "dest": "staticfiles_build/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "CodingIndiaErp/wsgi.py"
        }
    ]
}